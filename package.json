{
  "name": "@form8ion/prettier-config",
  "description": "shareable config for prettier",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "type": "commonjs",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "repository": "form8ion/prettier-config",
  "bugs": "https://github.com/form8ion/prettier-config/issues",
  "homepage": "https://npm.im/@form8ion/prettier-config",
  "runkitExampleFilename": "./example.js",
  "sideEffects": false,
  "scripts": {
    "lint:lockfile": "lockfile-lint",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "run-s lint:js -- --fix",
    "lint:md": "remark . --frail",
    "generate:md": "remark . --output",
    "lint:peer": "npm ls >/dev/null",
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:publish": "publint --strict"
  },
  "files": [
    "example.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "dependencies": {
    "prettier": "^3.0.0"
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "1.0.65",
    "@form8ion/eslint-config": "7.0.6",
    "@form8ion/remark-lint-preset": "6.0.2",
    "ban-sensitive-files": "1.10.1",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "lockfile-lint": "4.12.1",
    "npm-run-all2": "6.1.1",
    "publint": "0.2.6",
    "remark-cli": "12.0.0",
    "remark-toc": "9.0.0"
  }
}
